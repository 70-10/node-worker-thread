  "devDependencies": {
    "ava": "0.16.0",
    "babel-cli": "6.16.0",
    "babel-eslint": "7.0.0",
    "babel-preset-latest": "6.16.0",
    "babel-register": "6.16.3",
    "del-cli": "0.2.0",
    "eslint": "3.7.1",
    "nyc": "8.3.1"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/70-10/node-worker-thread#readme",
  "keywords": [],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
