{
  "name": "worker-thread",
  "description": "Worker Thread Pattern for Node.js",
  "version": "0.4.0",
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "bugs": {
